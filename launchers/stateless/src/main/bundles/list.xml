    <!-- needed to read data from mime multipart requests -->
    <bundle>
      <groupId>com.sun.jersey.contribs</groupId>
      <artifactId>jersey-multipart</artifactId>
      <version>1.7</version>
    </bundle>
    <!--  dependency of jersey-multipart -->
    <bundle>
